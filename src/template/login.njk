            <div class="section-login">
                <section class="login-register">
                    <div class="login-register__title">
                        <h1>{{loginRegister.title}}
                        </h1>
                    </div>
                    <div class="login-register__separator"></div>
                    <div class="login-register__text">
                        <p class="body__quote">
                            <strong>{{loginRegister.quote}}</strong>
                        </p>
                        <p>{{loginRegister.helpTxt}}</p>
                    </div>
                    <form>
                        <div class="login-register__input">
                            <div class="login-register__email__input">
                                <label from="email-or-username">
                                    <strong>{{labelLogIn.emailUsername}}</strong>
                                </label>
                                <input id="email-or-username" name="email-or-username"/>
                            </div>
                            <div class="login-register__password__input">
                                <div class="password__input__text">
                                    <label from="password">
                                        <strong>{{labelLogIn.password}}</strong>
                                    </label>
                                    <a class="password__forget" href="{{labelLogIn.passwordHelpLink}}">
                                        <strong>{{labelLogIn.passwordHelp}}</strong>
                                    </a>
                                </div>
                                <input type="password" id="password" name="pwd">

                            </div>
                            <div class="login-register__submit__button">
                                <button type="submit">{{labelLogIn.button}}</button>
                            </div>
                        </div>
                    </form>
                </section>
            </div>
